<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".factura1">

    <CheckBox
        android:id="@+id/cBici"
        android:layout_width="142dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="51dp"
        android:text="Bicicleta"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/cMono"
        android:layout_width="142dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:text="Monopatín"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cPat" />

    <CheckBox
        android:id="@+id/cPat"
        android:layout_width="142dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="17dp"
        android:text="Patinete"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cBici" />

    <EditText
        android:id="@+id/eBici"
        android:layout_width="80dp"
        android:layout_height="54dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="52dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/cBici"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/eMono"
        android:layout_width="80dp"
        android:layout_height="54dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/cMono"
        app:layout_constraintTop_toBottomOf="@+id/ePat" />

    <EditText
        android:id="@+id/ePat"
        android:layout_width="80dp"
        android:layout_height="54dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toEndOf="@+id/cPat"
        app:layout_constraintTop_toBottomOf="@+id/eBici" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="71dp"
        android:layout_height="53dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="27dp"
        android:text="5€"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/eMono"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="71dp"
        android:layout_height="53dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="54dp"
        android:text="2€"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/eBici"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="71dp"
        android:layout_height="53dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="25dp"
        android:text="3€"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/ePat"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/btnIntento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="158dp"
        android:layout_marginTop="43dp"
        android:onClick="enviar"
        android:text="enviar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eMono" />

    <Button
        android:id="@+id/button35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="147dp"
        android:layout_marginEnd="47dp"
        android:onClick="Atras"
        android:text="atras"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />
</androidx.constraintlayout.widget.ConstraintLayout>